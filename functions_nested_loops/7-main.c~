#include "main.h"

/**
 * main - check the code for print_last_digit
 *
 * Return: Always 0.
 */
int main(void)
{
    int r;

    /* prueba con un número positivo */
    r = print_last_digit(98);
    _putchar(' ');
    
    /* prueba con cero */
    r = print_last_digit(0);
    _putchar(' ');

    /* prueba con número negativo */
    r = print_last_digit(-1024);
    _putchar(' ');

    /* prueba con otro número */
    r = print_last_digit(123456);
    _putchar('\n');

    return (0);
}
